{
  "files.trimTrailingWhitespace": true,
  "prettier.documentSelectors": ["**/*.gml"],
  "[gml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enableFiletypes": ["gml-gms2"],
  "cSpell.words": [
    "despawn",
    "despawned",
    "fullscreen",
    "irandom",
    "lengthdir",
    "lerp",
    "noone",
    "respawn",
    "wview",
    "xscale",
    "xview",
    "yscale",
    "yview"
  ]
}
